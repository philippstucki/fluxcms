        <parameter type="pipeline" name="xslt" value="../standard/plugins/aggregator/aggregator2rss.xsl"/>
        <plugin type="aggregator">
            <parameter name="mode" value="rss"/>
        </plugin>
    </plugins>
<plugins>
    
    
        <parameter name="xslt" type="pipeline" value="aggregator.xsl"/>
        <plugin type="navitree">
        </plugin>
        <plugin type="aggregator">
        </plugin>
    </plugins>
</bxcms>
